{
    "name": "ai-learning-server",
    "version": "1.0.0",
    "description": "Express server for AI learning platform",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "tsx watch src/server.ts",
        "build": "tsc && npm run copy:schema",
        "copy:schema": "mkdir -p dist/database && cp src/database/schema.sql dist/database/schema.sql",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "better-sqlite3": "^12.4.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "multer": "^1.4.5-lts.1",
        "node-html-to-image": "^3.4.0",
        "openai": "^6.7.0",
        "pdf-parse": "^1.1.3",
        "pdfjs-dist": "^5.4.394",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.0",
        "@types/pdf-parse": "^1.1.5",
        "tsx": "^4.6.0",
        "typescript": "^5.3.0"
    }
}
